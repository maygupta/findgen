<div class="modal fade" id="assignVendor" tabindex="-1" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">Accept Booking</h4>
      </div>
      <div class="modal-body">

        <div class="form-group">
          <label for="sel1">Vendor</label>
          <select  class="form-control" id="vendors">
          	<option selected disabled>Choose Vendor</option>
          <% @vendors.each do |vendor| %>
            <option value=<%=vendor.id%> > <%=vendor.name%> </option>
          <% end %>
          </select>
        </div>

        <div class="form-group">
          <label for="sel1">Operator</label>
          <select  class="form-control" id="operators">
          </select>
        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" id="accept_booking">Save</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->